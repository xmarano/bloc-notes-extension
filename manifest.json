{
    "manifest_version": 3,
    "name": "Bloc-notes",
    "version": "1.0.0",
    "description": "A quick, easy-to-access notepad in your browser.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        }
    },
    "permissions": ["storage"]
}
